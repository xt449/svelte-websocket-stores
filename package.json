{
	"name": "svelte-websocket-stores",
	"version": "0.1.1",
	"license": "MIT",
	"repository": "https://github.com/xt449/svelte-websocket-stores",
	"keywords": [
		"svelte",
		"websocket"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
	},
	"exports": {
		"./websocket": {
			"types": "./dist/websocket-wrapper.d.ts",
			"svelte": "./dist/websocket-wrapper.js"
		},
		"./store": {
			"types": "./dist/store.d.ts",
			"svelte": "./dist/store.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"publint": "^0.1.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.11"
	},
	"type": "module"
}
